                            android:button="@null"
                            android:gravity="center"

                            android:text="异常"
                            android:textColor="@color/radiobutton_red_select"
                            android:textSize="30px" />


                    </RadioGroup>

                </LinearLayout>

                <include layout="@layout/divider_gray_hor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="85px"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/bg_username"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="空开标识:"
                        android:textColor="@color/black"
                        android:textSize="30px" />

                    <include layout="@layout/fill_view" />

                    <RadioGroup
                        android:id="@+id/airswitch_marking"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/asf_ok"
                            android:layout_width="170px"
                            android:layout_height="54px"
                            android:layout_marginRight="10px"
                            android:background="@drawable/radiobutton_green_check_selector"
                            android:button="@null"
                            android:gravity="center"

                            android:text="正常"
                            android:textColor="@color/radiobutton_green_select"
                            android:textSize="30px" />

                        <RadioButton
                            android:id="@+id/asf_false"
                            android:layout_width="170px"
                            android:layout_height="54px"
                            android:layout_marginRight="10px"
                            android:background="@drawable/radiobutton_red_check_selector"
                            android:button="@null"
                            android:gravity="center"

                            android:text="异常"
                            android:textColor="@color/radiobutton_red_select"
                            android:textSize="30px" />

                    </RadioGroup>

                </LinearLayout>

                <include layout="@layout/divider_gray_hor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="85px"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/bg_username"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="备用通道余量:"
                        android:textColor="@color/black"
                        android:textSize="30px" />

                    <include layout="@layout/fill_view" />

                    <RadioGroup
                        android:id="@+id/sparechannel_margin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/bc_ok"
                            android:layout_width="170px"
                            android:layout_height="54px"
                            android:layout_marginRight="10px"
                            android:background="@drawable/radiobutton_green_check_selector"
                            android:button="@null"
                            android:gravity="center"

                            android:text="充足"
                            android:textColor="@color/radiobutton_green_select"
                            android:textSize="30px" />

                        <RadioButton
                            android:id="@+id/bc_false"
                            android:layout_width="170px"
                            android:layout_height="54px"
                            android:layout_marginRight="10px"
                            android:background="@drawable/radiobutton_red_check_selector"
                            android:button="@null"
                            android:gravity="center"

                            android:text="不足"
                            android:textColor="@color/radiobutton_red_select"
                            android:textSize="30px" />

                    </RadioGroup>

                </LinearLayout>

                <include layout="@layout/divider_gray_hor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="85px"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/bg_username"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="鼠患:"
                        android:textColor="@color/black"
                        android:textSize="30px" />

                    <include layout="@layout/fill_view" />

                    <RadioGroup
                        android:id="@+id/rats"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/mouse_ok"
                            android:layout_width="170px"
                            android:layout_height="54px"
                            android:layout_marginRight="10px"
                            android:background="@drawable/radiobutton_green_check_selector"
                            android:button="@null"
                            android:gravity="center"

                            android:text="不存在"
                            android:textColor="@color/radiobutton_green_select"
                            android:textSize="30px" />

                        <RadioButton
                            android:id="@+id/mouse_false"
                            android:layout_width="170px"
                            android:layout_height="54px"
                            android:layout_marginRight="10px"
                            android:background="@drawable/radiobutton_red_check_selector"
                            android:button="@null"
                            android:gravity="center"

                            android:text="存在"
                            android:textColor="@color/radiobutton_red_select"
                            android:textSize="30px" />

                    </RadioGroup>

                </LinearLayout>

                <include layout="@layout/divider_gray_hor" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:layout_marginTop="0dp"
                    android:background="@color/white"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="问题及建议:"
                        android:textColor="@color/black"
                        android:textSize="30px" />


                    <EditText
                        android:id="@+id/suggestion"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:gravity="left|center_vertical"
                        android:hint="备注"
                        android:imeOptions="actionDone"
                        android:inputType="textMultiLine"
                        android:maxLines="5"
                        android:textColor="@color/black"
                        android:textSize="30px" />

                </LinearLayout>

                <include layout="@layout/divider_gray_hor" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginTop="10dp"
                    android:background="@color/white"
                    android:gravity="top|left"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="附件:"
                        android:textColor="@color/black"
                        android:textSize="30px" />

                    <ImageView
                        android:id="@+id/iv_add1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:gravity="left|top" />


                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="132px" />
            </LinearLayout>

        </ScrollView>

        <include layout="@layout/bottom_tab" />
    </RelativeLayout>

</LinearLayout>

